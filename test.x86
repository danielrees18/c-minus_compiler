(FUNCTION  doAddition  [(void void)]
  (BB 1
    (OPER 14 Mov [(r 1)]  [(m EDI)])
  )
  (BB 2
    (OPER 4 Mov [(r 5)]  [(i 4)])
    (OPER 5 Mov [(r 3)]  [(r 5)])
    (OPER 6 Mov [(r 8)]  [(i 9)])
    (OPER 15 Mov [(m EDI)]  [(r 8)])
    (OPER 8 JSR []  [(s getValue)])
    (OPER 9 Mov [(r 9)]  [(m EAX)])
    (OPER 16 Mov [(r 7)]  [(r 3)])
    (OPER 10 Add_I [(r 7)]  [(r 7)(r 0)])
    (OPER 11 Mov [(r 2)]  [(r 7)])
    (OPER 12 Mov [(m EAX)]  [(r 2)])
  )
  (BB 0
    (OPER 2 Return []  [(m EAX)])
  )
)
(FUNCTION  getValue  [(int x)]
  (BB 1
    (OPER 6 Mov [(r 1)]  [(m EDI)])
  )
  (BB 2
    (OPER 4 Mov [(m EAX)]  [(r 1)])
  )
  (BB 0
    (OPER 2 Return []  [(m EAX)])
  )
)
