(FUNCTION  doLoop  [(void void)]
  (BB 1
    (OPER 13 Mov [(r 1)]  [(m EDI)])
  )
  (BB 2
    (OPER 4 Mov [(r 4)]  [(i 0)])
    (OPER 14 Cmp [(m Flags)]  [(r 2)(r 4)])
    (OPER 15 BNE []  [(bb 4)(m Flags)])
  )
  (BB 3
    (OPER 7 Mov [(r 7)]  [(i 1)])
    (OPER 16 Mov [(r 6)]  [(r 2)])
    (OPER 8 Add_I [(r 6)]  [(r 6)(r 7)])
    (OPER 9 Mov [(r 2)]  [(r 6)])
    (OPER 17 Cmp [(m Flags)]  [(r 3)(i 0)])
    (OPER 10 BNE []  [(bb 3)(m Flags)()
])
  )
  (BB 4
    (OPER 11 Mov [(m EAX)]  [(r 2)])
  )
  (BB 0
    (OPER 2 Return []  [(m EAX)])
  )
)
